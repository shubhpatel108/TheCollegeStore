<div class="container_4">
    <div class="innertitle">
        <p>Latest Books</p>
    </div>
    <div class="no-touch">
      <ul class="grid cs-style-3">
        <% @latest.each do |book| %>
          <%= render :partial => "shared/book_tile", :locals => {:book => book}%>
        <% end %>
      </ul>
    </div>
</div>

<div class="container_4">
    <div class="innertitle">
        <p>Popular In Your College</p>
    </div>
    <div class="no-touch">
      <ul class="grid cs-style-3">
        <% @popular.each do |book| %>
          <%= render :partial => "shared/book_tile", :locals => {:book => book}%>
        <% end %>
      </ul>
    </div>
</div>
